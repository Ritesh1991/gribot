<?xml version="1.0"?>

<!-- Launch file for move_base -->

<launch>


   <!-- Run the navigation -->
   
   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
      <rosparam file="$(find gribot)/config/common_costmap_params.yaml" command="load" ns="global_costmap" />
      <rosparam file="$(find gribot)/config/common_costmap_params.yaml" command="load" ns="local_costmap" />
      <rosparam file="$(find gribot)/config/local_costmap_params.yaml" command="load" />
      <rosparam file="$(find gribot)/config/global_costmap_params.yaml" command="load" />
      <rosparam file="$(find gribot)/config/base_local_planner_params.yaml" command="load" />
      <param name="controller_frequency" type="double" value="10" /> <!-- Default value is 20Hz -->
   </node>

</launch>